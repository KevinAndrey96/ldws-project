<script type="text/javascript">
var Contador=1;
var Cont=1;
var Cont_fisico=0;
var Cont_subred=0;

    <%if params[:edit]=="true"%>
      <% @physicals.each do |physical| %>
        Cont_fisico= Cont_fisico+1;
      <% end %> 
    <% end %>
    
    <%if params[:edit]=="true"%>
      <% @subnets.each do |subnet| %>
        Cont_subred= Cont_subred+1;
      <% end %> 
    <% end %>

    $( "#add" ).click(function() {
          <%if params[:edit]=="true"%>
              $('#delete_physical_'+Cont_fisico).hide();
              if (Cont_fisico+1 > 10){
                $("#myModal1").modal("show");
              } else {
                $('#delete_physical_'+Cont_fisico).hide();
                Cont_fisico= Cont_fisico+1;
                $('#physical_'+Cont_fisico).show();
                document.getElementById("fisico_"+Cont_fisico).value= "true";       
              }
          <% else %>
              if (Contador+1 > 10){
                $("#myModal1").modal("show");
              } else {
                $('#delete_physical_'+Contador).hide();
                Contador= Contador+1;
                $('#physical_'+Contador).show();
                document.getElementById("fisico_"+Contador).value= "true";       
              }        
          <% end %>  
    });
    
    
    $( "#add_subred" ).click(function() {
          <%if params[:edit]=="true"%>
              $('#delete_subnet_'+Cont_subred).hide();
              if (Cont_subred+1 > 10){
                $("#myModal3").modal("show");
