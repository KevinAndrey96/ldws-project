    function logicalBtn(){
      
      
        //Validar IP
        var cantidad_pisos = 0;
        if (document.getElementById('physical_computers_1').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_1').value);
        if (document.getElementById('physical_computers_2').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_2').value);
        if (document.getElementById('physical_computers_3').value != "")
