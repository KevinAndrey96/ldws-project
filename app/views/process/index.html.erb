        if (document.getElementById('physical_computers_4').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_4').value);        
        if (document.getElementById('physical_computers_5').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_5').value);
        if (document.getElementById('physical_computers_6').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_6').value);        
        if (document.getElementById('physical_computers_7').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_7').value);
        if (document.getElementById('physical_computers_8').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_8').value);        
        if (document.getElementById('physical_computers_9').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_9').value);
        if (document.getElementById('physical_computers_10').value != "")
          cantidad_pisos = cantidad_pisos + parseInt(document.getElementById('physical_computers_10').value);       
    
        var min = 0;
        var max = 0;
        //CLASE C
        if (cantidad_pisos >= 0 && cantidad_pisos <= 254 ){
          min = 192;
          max = 223;
          document.getElementById('logical_host1').min = '192';
          document.getElementById('logical_host1').max = '223';
          document.getElementById('logical_host1').value = '192';
          document.getElementById('logical_host2').value = '0';
          document.getElementById('logical_host3').value = '0';
          document.getElementById('logical_host4').value = '0';
        }
        //CLASE B
        if (cantidad_pisos >= 255 && cantidad_pisos <= 65534 ){
          min = 128;
          max = 191;
          document.getElementById('logical_host1').min = '128';
          document.getElementById('logical_host1').max = '191'; 
          document.getElementById('logical_host1').value = '128';
          document.getElementById('logical_host2').value = '0';
          document.getElementById('logical_host3').value = '0';
          document.getElementById('logical_host4').value = '0';
        }
        //CLASE A
        if (cantidad_pisos >= 65535 && cantidad_pisos <= 16777214 ){
          min = 0;
          max = 127;
          document.getElementById('logical_host1').min = '0';
          document.getElementById('logical_host1').max = '127';
          document.getElementById('logical_host1').value = '0';
          document.getElementById('logical_host2').value = '0';
          document.getElementById('logical_host3').value = '0';
          document.getElementById('logical_host4').value = '0';
        }
        
        document.getElementById("logical_host1").onkeyup = function() {
          this.value = minmax(this.value, min, max)
        };
        document.getElementById("logical_host1").placeholder = min;

    }
    
