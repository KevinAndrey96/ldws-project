<p id="notice"><%= notice %></p>

<h1>Listing Switches</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Reference</th>
      <th>Price</th>
      <th>Nports</th>
      <th>Port</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @switches.each do |switch| %>
      <tr>
        <td><%= switch.name %></td>
        <td><%= switch.reference %></td>
        <td><%= switch.price %></td>
        <td><%= switch.nports %></td>
        <td><%= switch.port %></td>
        <td><%= link_to 'Show', switch %></td>
        <td><%= link_to 'Edit', edit_switch_path(switch) %></td>
        <td><%= link_to 'Destroy', switch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Switch', new_switch_path %>
