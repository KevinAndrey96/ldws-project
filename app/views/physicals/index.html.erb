<p id="notice"><%= notice %></p>

<h1>Listing Physicals</h1>

<table>
  <thead>
    <tr>
      <th>Company</th>
      <th>Height</th>
      <th>Width</th>
      <th>Large</th>
      <th>Computers</th>
      <th>Conference hall</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @physicals.each do |physical| %>
      <tr>
        <td><%= physical.company %></td>
        <td><%= physical.height %></td>
        <td><%= physical.width %></td>
        <td><%= physical.large %></td>
        <td><%= physical.computers %></td>
        <td><%= physical.conference_hall %></td>
        <td><%= link_to 'Show', physical %></td>
        <td><%= link_to 'Edit', edit_physical_path(physical) %></td>
        <td><%= link_to 'Destroy', physical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Physical', new_physical_path %>
