<div class="col-md-3"></div>
<div class="col-md-6">
<h1>Resumen de Usuario</h1>
<p>
  <strong>Nombre:</strong>
  <%= @user.name %>
</p>
<p>
  <strong>Teléfono:</strong>
  <%= @user.phone %>
</p>
<p>
  <strong>Correo electrónico:</strong>
  <%= @user.email %>
</p>
<p>
  <strong>Rol:</strong>
  <%= @user.role %>
</p>
<p>
  <strong>Registro:</strong>
  <%= @user.created_at.localtime %>
</p>
<p>
  <strong>Ultimo ingreso:</strong>
  <%= @user.updated_at %>
</p>



<br>
<%= link_to 'Atrás', manage_users_path, :class => "btn btn-default" %>
</div>
<div class="col-md-3"></div>