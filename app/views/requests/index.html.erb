<p id="notice"><%= notice %></p>

<h1>Listing Requests</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Company</th>
      <th>Description</th>
      <th>Objective</th>
      <th>Observation</th>
      <th>Web</th>
      <th>Bd</th>
      <th>Aplications</th>
      <th>Proxy</th>
      <th>Voip</th>
      <th>Video conference</th>
      <th>Active dir</th>
      <th>Dhcp</th>
      <th>Dns</th>
      <th>Ftp</th>
      <th>email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.title %></td>
        <td><%= request.company %></td>
        <td><%= request.description %></td>
        <td><%= request.objective %></td>
        <td><%= request.observation %></td>
        <td><%= request.web %></td>
        <td><%= request.bd %></td>
        <td><%= request.aplications %></td>
        <td><%= request.proxy %></td>
        <td><%= request.voip %></td>
        <td><%= request.video_conference %></td>
        <td><%= request.active_dir %></td>
        <td><%= request.dhcp %></td>
        <td><%= request.dns %></td>
        <td><%= request.ftp %></td>
        <td><%= request.email %></td>
        
        
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>
