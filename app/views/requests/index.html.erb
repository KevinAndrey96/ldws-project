<br>
<p id="notice"><%= notice %></p>
<h1>Solicitudes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Título</th>
      <th>Empresa</th>
      <th>Descripción</th>
      <th>Objetivo</th>
      <th>Observaciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.title %></td>
        <td><%= request.company.name %></td>
        <td><%= request.description %></td>
        <td><%= request.objective %></td>
        <td><%= request.observation %></td>
        <td><%= link_to 'Ver', panel_index_path(:request_id => request.id) %></td>
       
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Solicitud', new_request_path %>
